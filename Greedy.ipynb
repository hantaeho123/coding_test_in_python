   "source": [
    "# 반복되는 수열 \n",
    "\n",
    "# 입력 받기\n",
    "N, M, K = map(int , input(\"N M K\").split())\n",
    "input_list = list(map(int, input(\"N개의 수들\").split()))\n",
    "\n",
    "# 정렬된 입력들 \n",
    "input_list.sort(reverse = True)\n",
    "\n",
    "first = input_list[0]\n",
    "second = input_list[1]\n",
    "\n",
    "\n",
    "# 수열로 결과값 구하기 \n",
    "result = (first * K + second) * (M // (K+1)) + first * (M % (K+1))\n",
    "print(result)"
   ]
