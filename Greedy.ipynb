   ],
   "source": [
    "# 입력 받기\n",
    "N, M, K = map(int , input(\"N M K\").split())\n",
    "input_list = list(map(int, input(\"N개의 수들\").split()))\n",
    "\n",
    "# 정렬된 입력들 \n",
    "input_list.sort(reverse = True)\n",
    "\n",
    "# 결과 만들기 \n",
    "output = 0\n",
    "val = 0\n",
    "for i in range(M) :\n",
    "    if (val < K) :\n",
    "        output += input_list[0]\n",
    "        val += 1\n",
    "    else :\n",
    "        output += input_list[1]\n",
    "        val = 0 \n",
    "\n",
    "print(output)"
   ]
