   "source": [
    "N, K = map(int, input().split())\n",
    "\n",
    "count = 0\n",
    "\n",
    "while True :\n",
    "    if (N == 1) :\n",
    "        break\n",
    "    elif (N % K == 0) :\n",
    "        N //= K\n",
    "        count += 1\n",
    "    else :\n",
    "        N -= 1\n",
    "        count += 1\n",
    "print(count)"
   ]
  },
